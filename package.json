{
  "name": "@stomp/stompjs",
  "version": "0.0.1",
  "description": "STOMP cleint for Javascript and Typescript",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf _bundles lib lib-esm",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack && webpack --mode=production --output-filename=stomp.min.js",
    "test": "qunit --timeout 10000 -c tests/config/node-config.js -t tests/unit/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stomp-js/stompjs.git"
  },
  "keywords": [
    "STOMP",
    "RabbitMQ",
    "ActiveMQ",
    "Websocket",
    "SockJS"
  ],
  "author": "deepak@kreatio.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stomp-js/stompjs/issues"
  },
  "homepage": "https://github.com/stomp-js/stompjs#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "jquery": "^3.3.1",
    "qunit": "^1.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "websocket": "^1.0.26"
  }
}
